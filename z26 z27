                marka: 'nissan',
                model: 'micra',
                przebieg: '10000',
                pokaz_samochod: function()
                {
                    document.write("Samochod Marka: "+this.marka+" Model: "+this.model+" Przebieg: "+this.przebieg);
                }
            } 
            samochod.pokaz_samochod();
            samochod.datap='2010 rok';
            samochod.wypisz=function(){document.write(" Rok produkcji: "+this.datap+"<br>");}
            samochod.wypisz();
            
